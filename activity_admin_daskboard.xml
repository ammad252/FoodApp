<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dash"
    android:fillViewport="true"
    tools:context=".Admin_Daskboard">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">

        <ViewFlipper
            android:id="@+id/flipper"

            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:layout_marginTop="@dimen/_40sdp" />
    </LinearLayout>

    <LinearLayout


        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/_30sdp"
        android:orientation="horizontal">

        <!--            1st CardView-->
        <androidx.cardview.widget.CardView
            android:layout_width="145dp"
            android:layout_height="145dp"
            android:layout_marginLeft="@dimen/_20sdp"
            app:cardBackgroundColor="@color/White"
            app:cardCornerRadius="20dp"
            android:elevation="10dp">
            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/detail"
                app:lottie_autoPlay="true"
                android:layout_width="145dp"
                android:layout_height="140dp"
                app:lottie_fileName="67221-food-market-app-interaction.json"
                app:lottie_loop="true"/>
            <!--            <ImageView-->
            <!--        -->
            <!--                android:layout_width="wrap_content"-->
            <!--                android:layout_height="125dp"-->
            <!--                app:srcCompat="@drawable/diethistroy" />-->

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="OrderDetail"
                android:layout_marginTop="124dp"
                android:layout_marginLeft="18dp"
                android:textStyle="bold"
                android:textColor="@color/black" />

        </androidx.cardview.widget.CardView>
        <!--            2nd CardView-->
        <androidx.cardview.widget.CardView
            android:layout_width="145dp"
            android:layout_height="145dp"
            android:layout_marginLeft="20dp"
            app:cardBackgroundColor="@color/White"
            app:cardCornerRadius="20dp"
            android:elevation="10dp">

            <com.airbnb.lottie.LottieAnimationView

                android:id="@+id/dish"
                app:lottie_autoPlay="true"
                android:layout_width="145dp"
                android:layout_height="140dp"
                app:lottie_fileName="47294-view-hide-icon.json"
                app:lottie_loop="true"/>

            <!--            <ImageView-->
            <!--               -->
            <!--                android:layout_width="wrap_content"-->
            <!--                android:layout_height="125dp"-->
            <!--                app:srcCompat="@drawable/nutrition" />-->
